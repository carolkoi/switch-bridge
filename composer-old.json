{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "barryvdh/laravel-cors": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "barryvdh/laravel-snappy": "^0.4.7",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "~6.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "hwi/oauth-bundle": "^1.1",
        "infyomlabs/adminlte-templates": "5.8.x-dev",
        "infyomlabs/laravel-generator": "5.8.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "kartik-v/bootstrap-star-rating": "dev-master",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8.0",
        "league/fractal": "^0.18.0",
        "maatwebsite/excel": "^3.1",
        "php-http/guzzle6-adapter": "^2.0",
        "php-http/httplug-bundle": "^1.18",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^3.11",
        "webpatser/laravel-uuid": "^3.0",
        "yajra/laravel-datatables-buttons": "^4.6",
        "yajra/laravel-datatables-html": "^4.4",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "appointer/swaggervel": "^2.5",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "WizPack\\Workflow\\": "packages/wizpack/workflow/src"
        },
        "files": [
            "packages/wizpack/workflow/src/Helpers/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
